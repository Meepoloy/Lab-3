@model Lab3.Model.Student;

@{
    Layout = "_Layout";
    ViewData["Title"] = "DisplayStudent";
}
<h2>Your Favorite Student</h2>
<table>
    <tr>
        <td>
            First Name:
        </td>
        <td>
            @Model.firstName
        </td>
    </tr>
    <tr>
        <td>
            Last Name:
        </td>
        <td>
            @Model.lastName
        </td>
    </tr>
    <tr>
        <td>
            Student Id:
        </td>
        <td>
            @Model.studId
        </td>
    </tr>
    <tr>
        <td>
            Email Address:
        </td>
        <td>
            @Model.emailAdd
        </td>
    </tr>
    <tr>
        <td>
            Description:
        </td>
        <td>
           @Model.description
        </td>
    </tr>
</table>